{
  "name": "cicada",
  "type": "module",
  "homepage": "https://github.com/symphoniacloud/cicada",
  "license": "MIT",
  "author": {
    "email": "mike@symphonia.io",
    "name": "Mike Roberts",
    "url": "https://symphonia.io"
  },
  "scripts": {
    "local-checks": "npx tsc && npm run lint && npm run format && npm run local-tests",
    "lint": "eslint --max-warnings=0 --fix-dry-run \"{src,test}/**/*.{js,ts}\"",
    "format": "prettier --check \"{src,test}/**/*.{js,ts}\"",
    "format-fix": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "local-tests": "npx vitest run --dir test/local --config test/local/vitest.config.mjs",
    "remote-tests": "npx vitest run --dir test/remote --config test/remote/vitest.config.mjs",
    "pre-deploy-check": "npx tsc",
    "deploy-cdk": "cd src/cdk && npx cdk deploy",
    "--COMMENT--": "-- \"$@\" below passes original parameters through to deploy-cdk",
    "deploy": "npm run pre-deploy-check && npm run deploy-cdk -- \"$@\""
  },
  "devDependencies": {
    "@tsconfig/node22": "22.x",
    "@types/aws-lambda": "8.x",
    "@types/deep-equal": "1.x",
    "@types/node": "22.x",
    "@types/web-push": "3.x",
    "aws-cdk": "2.x",
    "aws-cdk-lib": "2.x",
    "constructs": "10.x",
    "deep-equal": "2.x",
    "dotenv": "17.x",
    "eslint": "9.x",
    "eslint-config-prettier": "10.x",
    "prettier": "3.x",
    "tsx": "4.x",
    "typescript": "5.x",
    "typescript-eslint": "8.x",
    "vitest": "3.x"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "2.x",
    "@aws-lambda-powertools/metrics": "2.x",
    "@aws-lambda-powertools/parameters": "2.x",
    "@aws-lambda-powertools/parser": "2.x",
    "@aws-lambda-powertools/tracer": "2.x",
    "@aws-sdk/client-dynamodb": "3.x",
    "@aws-sdk/client-eventbridge": "3.x",
    "@aws-sdk/client-lambda": "3.x",
    "@aws-sdk/client-s3": "3.x",
    "@aws-sdk/client-ssm": "3.x",
    "@aws-sdk/lib-dynamodb": "3.x",
    "@middy/core": "4.x",
    "@octokit/auth-app": "6.x",
    "@octokit/rest": "20.x",
    "@symphoniacloud/dynamodb-entity-store": "2.1.0-alpha.9",
    "@symphoniacloud/dynamodb-entity-store-testutils": "2.1.0-alpha.9",
    "@symphoniacloud/hiccough": "0.1.0-alpha.3",
    "unzipper": "0.x",
    "url-pattern": "1.x",
    "web-push": "3.x",
    "zod": "4.x"
  }
}
